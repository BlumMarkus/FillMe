<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="de.me.fill.mblum.android.fillme.MenuActivity">

    <TextView
        android:id="@+id/txt_TitleActualMileage"
        android:layout_width="130dp"
        android:layout_height="18dp"
        android:layout_marginLeft="12dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="25dp"
        android:text="@string/str_TitleActualMileage"
        android:textSize="16sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_ActualMileage"
        android:layout_width="wrap_content"
        android:layout_height="32dp"
        android:layout_marginLeft="30dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="13dp"
        android:textSize="28sp"
        app:layout_constraintLeft_toRightOf="@+id/txt_TitleActualMileage"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_UnitActualMileage"
        android:layout_width="wrap_content"
        android:layout_height="18dp"
        android:layout_marginLeft="5dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="25dp"
        android:text="@string/str_UnitActualMileage"
        app:layout_constraintLeft_toRightOf="@+id/txt_ActualMileage"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_TitleLastEntry"
        android:layout_width="130dp"
        android:layout_height="18dp"
        android:layout_marginLeft="12dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="25dp"
        android:text="@string/str_TitleLastEntry"
        android:textSize="16sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_TitleActualMileage" />

    <TextView
        android:id="@+id/txt_LastEntry"
        android:layout_width="wrap_content"
        android:layout_height="32dp"
        android:layout_marginLeft="30dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="13dp"
        android:textSize="28sp"
        app:layout_constraintLeft_toRightOf="@+id/txt_TitleLastEntry"
        app:layout_constraintTop_toBottomOf="@+id/txt_ActualMileage" />

    <TextView
        android:id="@+id/txt_TitleActualConsumption"
        android:layout_width="130dp"
        android:layout_height="18dp"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="30dp"
        android:text="@string/str_TitleActualConsumption"
        android:textSize="16sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_showEntry"
        android:layout_marginStart="12dp" />

    <TextView
        android:id="@+id/txt_ActualConsumption"
        android:layout_width="wrap_content"
        android:layout_height="32dp"
        android:layout_marginLeft="30dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="18dp"
        android:textSize="28sp"
        app:layout_constraintLeft_toRightOf="@+id/txt_TitleActualConsumption"
        app:layout_constraintTop_toBottomOf="@+id/btn_showEntry" />

    <TextView
        android:id="@+id/txt_UnitActualConsumption"
        android:layout_width="wrap_content"
        android:layout_height="18dp"
        android:layout_marginLeft="5dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="30dp"
        android:text="@string/str_UnitActualConsumption"
        app:layout_constraintLeft_toRightOf="@+id/txt_ActualConsumption"
        app:layout_constraintTop_toBottomOf="@+id/btn_showEntry" />

    <TextView
        android:id="@+id/txt_TitleActualConsumptionCost"
        android:layout_width="130dp"
        android:layout_height="18dp"
        android:layout_marginLeft="12dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="25dp"
        android:text="@string/str_TitleConsumptionCost"
        android:textSize="16sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_TitleActualConsumption" />

    <TextView
        android:id="@+id/txt_ActualConsumptionCost"
        android:layout_width="wrap_content"
        android:layout_height="32dp"
        android:layout_marginLeft="30dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="11dp"
        android:textSize="28sp"
        app:layout_constraintLeft_toRightOf="@+id/txt_TitleActualConsumptionCost"
        app:layout_constraintTop_toBottomOf="@+id/txt_ActualConsumption" />

    <TextView
        android:id="@+id/txt_UnitActualConsumptionCost"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="25dp"
        android:text="@string/str_UnitConsumptionCost"
        app:layout_constraintLeft_toRightOf="@+id/txt_ActualConsumptionCost"
        app:layout_constraintTop_toBottomOf="@+id/txt_UnitActualConsumption" />

    <TextView
        android:id="@+id/txt_TitleTotalConsumption"
        android:layout_width="130dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="30dp"
        android:text="@string/str_TitleTotalConsumption"
        android:textSize="16sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_DataLastFill"
        android:layout_marginStart="12dp" />

    <TextView
        android:id="@+id/txt_TotalConsumption"
        android:layout_width="wrap_content"
        android:layout_height="32dp"
        android:layout_marginLeft="30dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="18dp"
        android:textSize="28sp"
        app:layout_constraintLeft_toRightOf="@+id/txt_TitleTotalConsumption"
        app:layout_constraintTop_toBottomOf="@+id/btn_DataLastFill" />

    <TextView
        android:id="@+id/txt_UnitTotalConsumption"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="30dp"
        android:text="@string/str_UnitTotalConsumption"
        app:layout_constraintLeft_toRightOf="@+id/txt_TotalConsumption"
        app:layout_constraintTop_toBottomOf="@+id/btn_DataLastFill" />

    <TextView
        android:id="@+id/txt_TitletotalConsumptionCost"
        android:layout_width="130dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="12dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="25dp"
        android:text="@string/str_TitleTotalConsumptionCost"
        android:textSize="16sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_TitleTotalConsumption" />

    <TextView
        android:id="@+id/txt_TotalConsumptionCost"
        android:layout_width="wrap_content"
        android:layout_height="32dp"
        android:layout_marginLeft="30dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="11dp"
        android:textSize="28sp"
        app:layout_constraintLeft_toRightOf="@+id/txt_TitletotalConsumptionCost"
        app:layout_constraintTop_toBottomOf="@+id/txt_TotalConsumption" />

    <TextView
        android:id="@+id/txt_UnitTotalConsumptionCost"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="25dp"
        android:text="@string/str_UnitTotalConsumptionCost"
        app:layout_constraintLeft_toRightOf="@+id/txt_TotalConsumptionCost"
        app:layout_constraintTop_toBottomOf="@+id/txt_UnitTotalConsumption" />

    <Button
        android:id="@+id/btn_newEntry"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="12dp"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="35dp"
        android:text="@string/str_newEntry"
        android:textSize="16sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_TitleLastEntry"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <Button
        android:id="@+id/btn_showEntry"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp"
        android:layout_marginTop="6dp"
        android:text="@string/str_showEntry"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_newEntry"
        app:layout_constraintHorizontal_bias="0.0"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="12dp"
        android:layout_marginEnd="12dp"
        tools:layout_constraintLeft_creator="1" />

    <Button
        android:id="@+id/btn_DataLastFill"
        android:layout_width="110dp"
        android:layout_height="55dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="35dp"
        android:text="@string/str_DataLastFill"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_TitleActualConsumptionCost" />

    <Button
        android:id="@+id/btn_DataLastMonth"
        android:layout_width="110dp"
        android:layout_height="55dp"
        android:layout_marginEnd="117dp"
        android:layout_marginLeft="117dp"
        android:layout_marginRight="117dp"
        android:layout_marginStart="117dp"
        android:text="@string/str_DataLastMonth"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/btn_DataLastYear"
        app:layout_constraintLeft_toLeftOf="@+id/btn_DataLastFill"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginTop="35dp"
        app:layout_constraintTop_toBottomOf="@+id/txt_TitleActualConsumptionCost" />

    <Button
        android:id="@+id/btn_DataLastYear"
        android:layout_width="110dp"
        android:layout_height="55dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:text="@string/str_DataLastYear"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="35dp"
        app:layout_constraintTop_toBottomOf="@+id/txt_TitleActualConsumptionCost" />

</android.support.constraint.ConstraintLayout>
